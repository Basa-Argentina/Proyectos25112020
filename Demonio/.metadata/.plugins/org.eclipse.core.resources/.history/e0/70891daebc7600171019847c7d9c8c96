import java.io.*;
public class Comandos {
    public static void main(String[] args) {
 
       
        String comando = "cmd /c ipconfig";
 
        try{
 
            // Ejecucion Basica del Comando
            Process proceso = Runtime.getRuntime().exec(comando);
 
         
               
        }catch (IOException e) {
                System.out.println("Excepción: ");
                e.printStackTrace();
        }
    }
}