	public static void Insertar(){
		
		File dir = new File("c:/Temp/FAE/LEE/");
		   
		String[] ficheros = dir.list();
		   
		if (ficheros == null)
			System.out.println("No hay ficheros en el directorio especificado");
		
		else { 
			for (int x=0;x<ficheros.length;x++){
				System.out.println(ficheros[x]);
				main1(ficheros[x]);
			}
		}
	}
	
	
  public static void main1 (String ficheros) {

    try {
    String Abrir = "c:/Temp/FAE/LEE/";
    String AbrirFile  = Abrir + ficheros ;
	//File fXmlFile = new File("c:/Temp/FAE/LEE/2011_12_186_4452.8.faeout");
	File fXmlFile = new File (AbrirFile);	
	DocumentBuilderFactory dbFactory = DocumentBuilderFactory.newInstance();
	DocumentBuilder dBuilder = dbFactory.newDocumentBuilder();
	Document doc = dBuilder.parse(fXmlFile);

	//optional, but recommended
	//read this - http://stackoverflow.com/questions/13786607/normalization-in-dom-parsing-with-java-how-does-it-work
	doc.getDocumentElement().normalize();

	System.out.println("Root element :" + doc.getDocumentElement().getNodeName());

	NodeList nList = doc.getElementsByTagName("FE");
//	NodeList nList1 = doc.getElementsByTagName("GUIDFactura");

	System.out.println("----------------------------");

	for (int temp = 0; temp < nList.getLength(); temp++) {

		Node nNode = nList.item(temp);
		//for (int temp1 = 0; temp1 < nList1.getLength(); temp1++) {
			//Node nNode1 = nList1.item(temp);
		

		System.out.println("\nCurrent Element :" + nNode.getNodeName());

		if (nNode.getNodeType() == Node.ELEMENT_NODE) {
			//if (nNode1.getNodeType() == Node.ELEMENT_NODE) {

			Element eElement = (Element) nNode;
			  Connection conn;
		        try {
		           String idcom  = (eElement.getElementsByTagName("IDComprobante").item(0).getTextContent());
		           int idcomlisto = Integer.parseInt(idcom);
		           String cae  = (eElement.getElementsByTagName("CAE").item(0).getTextContent());
		           String fechavtocae  = (eElement.getElementsByTagName("FechaVtoCAE").item(0).getTextContent());
		           String codAFIP  = (eElement.getElementsByTagName("CodigoAFIP").item(0).getTextContent());
		           String MERROR  = (eElement.getElementsByTagName("MotivoError").item(0).getTextContent());
		           String CERROR  = (eElement.getElementsByTagName("CodigoError").item(0).getTextContent());
		           String NCOMPRO  = (eElement.getElementsByTagName("NumeroComprobante").item(0).getTextContent());
		      //     String Nafip  = (eElement.getElementsByTagName("NotificacionAFIP").item(0).getTextContent());
		        	
		           Insertar1(idcom, idcomlisto, cae, fechavtocae, codAFIP, MERROR, CERROR, NCOMPRO);
		           /*  System.out.println(idcom);
