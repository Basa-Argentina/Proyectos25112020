			System.out.println("IDComprobante : " + eElement.getElementsByTagName("IDComprobante").item(0).getTextContent());
			System.out.println("CAE : " + eElement.getElementsByTagName("CAE").item(0).getTextContent());
			System.out.println("FechaVtoCAE : " + eElement.getElementsByTagName("FechaVtoCAE").item(0).getTextContent());
			System.out.println("CodigoAFIP : " + eElement.getElementsByTagName("CodigoAFIP").item(0).getTextContent());
			System.out.println("MotivoError : " + eElement.getElementsByTagName("MotivoError").item(0).getTextContent());
			System.out.println("CodigoError : " + eElement.getElementsByTagName("CodigoError").item(0).getTextContent());
